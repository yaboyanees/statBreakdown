<p class= "team"><%= @teamNames.join('') %></p>
<%= render partial: 'teamScript' %>
<div id="teamChart"></div>
<div id="teamData">
	<input class="rollup" name="newSeries" type="radio" value="a"> 2014</input>
	<input class="rollup" name="newSeries" type="radio" value="b"> 2013</input>
	<input class="rollup" name="newSeries" type="radio" value="c"> 2012</input>
	<input class="rollup" name="newSeries" type="radio" value="d"> 2011</input>
	<input class="rollup" name="newSeries" type="radio" value="e"> All</input>
</div>
<%= render partial: 'scheduleTable' %>
<div id="points" style="width: 49%; float: left; padding: 10px 0 40px 0"></div>
<div id="diff" style="width: 48%; float: left; float: left; padding: 10px 0 40px 0"></div>
<div id="diffDiv">
	<input class="difference" name="newSeries" type="radio" value="a"> 2014</input>
	<input class="difference" name="newSeries" type="radio" value="b"> 2013</input>
	<input class="difference" name="newSeries" type="radio" value="c"> 2012</input>
	<input class="difference" name="newSeries" type="radio" value="d"> 2011</input>
</div>
<%= render partial: 'pointsScript' %>
<%= render partial: 'diffScript' %>