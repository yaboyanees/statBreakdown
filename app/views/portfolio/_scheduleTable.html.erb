<div id="schedule">
	<button onclick="show14();return false"/>2014</button>
	<button onclick="show13();return false"/>2013</button>
	<button onclick="show12();return false"/>2012</button>
	<button onclick="show11();return false"/>2011</button>
	<div id="teamTable14" style="display:block">	
		<table cellspacing='0'> 
			<thead>
				<tr>
					<th colspan="3">2014 Schedule (<%= @win14 %>-<%= @loss14 %>)</th>
				</tr>
			</thead>
			<tbody>
				<% @record14.each do |r14| %>
					<% if r14[5].nil? %>
					<tr>
						<td><i>Wk <%= r14[1] %></i>:</td>
						<td ><b><%= r14[2] %></b></td>
						<td></td>
					</tr>	
					<% else %>
					<tr>
						<td><i>Wk <%= r14[1] %></i>:</td>	
						<td><b><%= r14[2] %></b> <%= r14[4] %>-<%= r14[5] %> vs <%= r14[3] %></td>
						<td><%= link_to 'Game Map', home_path(:id => r14[6]) %></td>
					</tr>					
					<% end %>
				<% end %>
			</tbody>
		</table>
	</div>
