<script> 
	function show14() { 
	document.getElementById("teamTable14").style.display="block"; 
	document.getElementById("teamTable13").style.display="none"; 
	document.getElementById("teamTable12").style.display="none"; 
	document.getElementById("teamTable11").style.display="none"; 
	} 
	function show13() { 
	document.getElementById("teamTable14").style.display="none"; 
	document.getElementById("teamTable13").style.display="block"; 
	document.getElementById("teamTable12").style.display="none"; 
	document.getElementById("teamTable11").style.display="none"; 
	} 
	function show12() { 
	document.getElementById("teamTable14").style.display="none"; 
	document.getElementById("teamTable13").style.display="none"; 
