<script type="text/javascript">
$(function () {
    $('#column').highcharts({
        chart: {
            type: 'column',
			backgroundColor:'rgba(255, 255, 255, 0.1)'
        },
        title: {
            text: '',
        },
		xAxis: {
			categories: ['Team', 'Offense', 'Defense']
		},
		yAxis: {
			title: {
				text: 'Season-Q',
				style: {
                    fontSize:'1.7vmax',
                }
			}
		},
        series: [{
            name: '<%= @team1name.first.gsub('"','') %>',
            data: <%= @teamValue1 %>,
            color: '#006E29'
        }, {
            name: '<%= @team2name.first.gsub('"','') %>',
            data: <%= @teamValue2 %>,
            color: '#333333'
        }]
    });
});
</script>